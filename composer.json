{
    "name": "rappasoft/laravel-5-boilerplate",
    "description": "The Laravel Boilerplate Project.",
    "keywords": [
        "framework",
        "laravel",
        "boilerplate"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "appstract/laravel-blade-directives": "^1.12",
        "arcanedev/log-viewer": "^8.0.2",
        "arcanedev/no-captcha": "^12.2",
        "barryvdh/laravel-dompdf": "^1.0",
        "barryvdh/laravel-translation-manager": "^0.6",
        "benjamincrozat/laravel-dropbox-driver": "^2.0",
        "beyondcode/laravel-self-diagnosis": "^1.5.0",
        "coderello/laravel-passport-social-grant": "^3.0.2",
        "consoletvs/invoices": "^1.8",
        "creativeorange/gravatar": "~1.0.20",
        "darryldecode/cart": "~4.2.0",
        "davejamesmiller/laravel-breadcrumbs": "5.3.2",
        "divineomega/laravel-password-exposed-validation-rule": "^2.5",
        "doctrine/dbal": "^2.13",
        "graham-campbell/markdown": "^14.0",
        "guzzlehttp/guzzle": "^7.2",
        "harimayco/laravel-menu": "^1.4.5",
        "hashids/hashids": "^4.1.0",
        "hieu-le/active": "^9.0",
        "jenssegers/agent": "^2.6",
        "kingflamez/laravelrave": "dev-dependabot/npm_and_yarn/postcss-7.0.39",
        "laravel/cashier": "^13.7",
        "laravel/framework": "^9.52",
        "laravel/helpers": "^1.5",
        "laravel/legacy-factories": "^1.2",
        "laravel/passport": "^10.3",
        "laravel/socialite": "^5.3",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.3",
        "laravelcollective/html": "^6.3",
        "laravelium/sitemap": "8.0.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/omnipay": "^3.2",
        "lexxyungcarter/chatmessenger": "^1.3",
        "maatwebsite/excel": "^3.1",
        "macsidigital/laravel-zoom": "^5.0",
        "madnest/madzipper": "^1.3",
        "mtownsend/read-time": "^2.0",
        "omnipay/paypal": "dev-test-ghactions",
        "omnipay/stripe": "^3.1",
        "paragonie/certainty": "^2.8",
        "razorpay/razorpay": "^2.5",
        "sendgrid/sendgrid": "~7",
        "skyraptor/chatter": "^0.5.11",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-html": "^3.1",
        "spatie/laravel-newsletter": "^4.11",
        "spatie/laravel-permission": "^5.5",
        "torann/geoip": "^3.0.1",
        "unisharp/laravel-filemanager": "^2.4",
        "yajra/laravel-datatables-buttons": "^4.10",
        "yajra/laravel-datatables-oracle": "^9.19"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.8",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "codedungeon/phpunit-result-printer": "^0.30",
        "filp/whoops": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.17",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.3",
        "phpunit/phpunit": "^9.5.10",
        "fakerphp/faker": "^1.9.1",
        "spatie/laravel-ignition": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan package:discover --ansi"
        ],
        "clear-all": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "composer dumpautoload -o"
        ],
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache"
        ],
        "phpunit": [
            "phpunit > output.txt -d memory_limit=2048M"
        ],
        "coverage-xml": [
            "phpunit --coverage-clover ./coverage.xml"
        ],
        "coverage-html": [
            "phpunit --coverage-html ./coverage"
        ],
        "coverage-text": [
            "phpunit --coverage-text"
        ],
        "format": [
            "./vendor/bin/php-cs-fixer fix app/  --show-progress=estimating",
            "./vendor/bin/php-cs-fixer fix config/  --show-progress=estimating",
            "./vendor/bin/php-cs-fixer fix database/  --show-progress=estimating",
            "./vendor/bin/php-cs-fixer fix resources/  --show-progress=estimating",
            "./vendor/bin/php-cs-fixer fix routes/  --show-progress=estimating",
            "./vendor/bin/php-cs-fixer fix tests/  --show-progress=estimating"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
